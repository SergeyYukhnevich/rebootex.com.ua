<footer>
    <div class="clearfix">
        <div class="col-xs-6">
            <div class="copyright">&copy; Rebootex
                <%
                var date = new Date();
                var year = date.getFullYear();
                %>
                <%= year %>
            </div>
        </div>
        <div class="col-xs-6 text-right">
            <ul class="social-links">
                <% for(social in data.socials) { %>
                    <li class="social <%= social %>">
                        <a href="<%= data.socials[social] %>" target="_blank"><i class="fa fa-<%= social %>"></i></a>
                    </li>
                <% } %>
            </ul>
        </div>
    </div>
</footer>